<?xml version="1.0" encoding="utf-8"?>
<mx:PieSeries xmlns:fx="http://ns.adobe.com/mxml/2009" 
			  xmlns:s="library://ns.adobe.com/flex/spark" 
			  xmlns:mx="library://ns.adobe.com/flex/mx"
			  fontSize="12" explodeRadius="0.1"
			  labelPosition="callout">
	<mx:fills>
		<mx:RadialGradient interpolationMethod="linearRGB">
			<mx:entries>
				<mx:GradientEntry color="0xFFAAAA" ratio="0.0"/>
				<mx:GradientEntry color="0xFF0000" ratio="1.0"/>
			</mx:entries>
		</mx:RadialGradient>
		<mx:RadialGradient interpolationMethod="linearRGB">
			<mx:entries>
				<mx:GradientEntry color="0xAAFFAA" ratio="0.0"/>
				<mx:GradientEntry color="0x00FF00" ratio="1.0"/>
			</mx:entries>
		</mx:RadialGradient>
		<mx:RadialGradient interpolationMethod="linearRGB">
			<mx:entries>
				<mx:GradientEntry color="0xAAAAFF" ratio="0.0"/>
				<mx:GradientEntry color="0x0000FF" ratio="1.0"/>
			</mx:entries>
		</mx:RadialGradient>
		<mx:RadialGradient interpolationMethod="linearRGB">
			<mx:entries>
				<mx:GradientEntry color="0xAAFFFF" ratio="0.0"/>
				<mx:GradientEntry color="0x00FFFF" ratio="1.0"/>
			</mx:entries>
		</mx:RadialGradient>
	</mx:fills>
	<mx:radialStroke>
		<s:SolidColorStroke color="0" weight="1"/>
	</mx:radialStroke>
	<mx:stroke>
		<s:SolidColorStroke color="0" weight="1"/>
	</mx:stroke>
	<mx:filters>
		<mx:BevelFilter shadowAlpha="0.5" highlightAlpha="0.5"/>
		<mx:DropShadowFilter alpha="0.5"/>
	</mx:filters>
</mx:PieSeries>
